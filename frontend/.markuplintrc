{
  "specs": {
    "\\.[jt]sx?$": "@markuplint/react-spec"
  },
  "parser": {
    "\\.[jt]sx?$": "@markuplint/jsx-parser"
  },
  "extends": [
    "markuplint:recommended"
  ],
  "rules": {
    "invalid-attr": {
      "options": {
        "allowAttrs": [
          "onTouchStart",
          "onTouchMove",
          "onTouchEnd"
        ]
      }
    }
  }
}
